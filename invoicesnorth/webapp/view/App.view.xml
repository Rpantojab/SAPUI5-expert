<mvc:View controllerName="logaligroup.invoicesnorth.controller.App"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
 



<App id="app" mobileWebAppCapable="true">
<pages>
	<Page
		title="{i18n>appTitle}"
		titleAlignment="Auto"
		titleLevel="Auto"
	>
	<headerContent>
		<Button icon="sap-icon://action" 
			    press=".onOpenDialogheader" 
				class="sapUiSmallMarginEnd"></Button>
	</headerContent>
		<content>
            <mvc:XMLView viewName="logaligroup.invoicesnorth.view.HelloPanel"></mvc:XMLView>
			
			<mvc:XMLView viewName="logaligroup.invoicesnorth.view.InvoicesList"></mvc:XMLView>
		</content>
	</Page>
</pages>
</App>	

</mvc:View>
