<mvc:View controllerName="logaligroup.invoicesnorth.controller.Invoices_north"
          xmlns:mvc="sap.ui.core.mvc" 
          displayBlock="true"
          xmlns="sap.m">
    <Page id="page" title="{i18n>title}"
		titleAlignment="Auto"
		titleLevel="Auto"
	>
	<headerContent>
		<Button icon="sap-icon://action" 
			    press=".onOpenDialogheader" 
				class="sapUiSmallMarginEnd"></Button>
	</headerContent>
		<content>
			<mvc:XMLView viewName="logaligroup.invoicesnorth.view.InvoicesList"></mvc:XMLView>
		</content>
    </Page>
</mvc:View>
